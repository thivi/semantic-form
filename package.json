{
    "name": "@codemachan/semantic-form",
    "version": "0.1.7",
    "description": "A React form library written to be used with Semantic UI",
    "keywords": [
        "React",
        "Form",
        "Semantic",
        "React JS",
        "form"
    ],
    "main": "dist/src/index.js",
    "types": "dist/typings/index.d.ts",
    "scripts": {
        "prebuild": "npm run clean",
        "build": "npm run compile",
        "clean": "rimraf ./dist",
        "compile": "run-script-os",
        "compile:win32": "node_modules\\.bin\\tsc -p tsconfig.json",
        "compile:default": "node_modules/.bin/tsc -p tsconfig.json",
        "lint": "eslint --ext .js,.ts .",
        "test": "jest",
        "prepublish": "npm run build"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.0.4",
        "@types/jest": "^24.0.0",
        "@types/react": "^16.9.0",
        "@typescript-eslint/eslint-plugin": "^4.1.1",
        "@typescript-eslint/parser": "^4.1.1",
        "classnames": "^2.2.6",
        "eslint": "^6.8.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-react": "^7.18.3",
        "eslint-plugin-react-hooks": "^4.0.0",
        "jest": "^24.0.0",
        "react": "^16.9.0",
        "react-dom": "^16.13.1",
        "rimraf": "^3.0.2",
        "run-script-os": "^1.0.7",
        "semantic-ui-react": "^0.87.3",
        "ts-jest": "^24.1.0",
        "typescript": "^3.9.7"
    },
    "peerDependencies": {
        "semantic-ui-react": "^0.87.3",
        "react": "^16.9.0",
        "@types/jest": "^24.0.0",
        "@types/react": "^16.9.0",
        "classnames": "^2.2.6"
    },
    "author": "Theviyanthan Krishnamohan",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thivi/semantic-form.git"
    },
    "bugs": {
        "url": "https://github.com/thivi/semantic-form/issues"
    },
    "homepage": "https://github.com/thivi/semantic-form#readme"
}
